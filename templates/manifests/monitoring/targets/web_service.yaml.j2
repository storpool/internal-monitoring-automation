{% for host in groups["all"] %}
{% if "healthchecks" in hostvars[host] and hostvars[host]["healthcheck_url"] | length %}
- targets:
{% for url in hostvars[host]["healthcheck_url"] %}
    - {{ url }}
{% endfor %}
  labels:
    instance: {{ host }}
    location: moonstone
    environment: production
{% endif %}
{% endfor %}
